{
  "name": "clevertap-expo-plugin",
  "version": "0.0.1",
  "description": "The CleverTap expo plugin enables integration of the CleverTap SDK into your Expo projects, to leverage CleverTap's features in your React Native applications.",
  "main": "./app.plugin.js",
  "scripts": {
    "build": "rm -rf build && tsc && cp -a support/serviceExtensionFiles build/support/ && cp -a support/contentExtensionFiles build/support/ && expo-module build",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "start": "expo start --dev-client",
    "clean": "expo-module clean",
    "lint": "expo-module lint",
    "test": "expo-module test",
    "prepare": "expo-module prepare",
    "prepublishOnly": "expo-module prepublishOnly",
    "expo-module": "expo-module",
    "open:ios": "xed example/ios",
    "open:android": "open -a \"Android Studio\" example/android"
  },
  "keywords": [
    "expo",
    "clevertap",
    "push",
    "notifications"
  ],
  "author": "CleverTap",
  "license": "MIT",
  "homepage": "https://github.com/CleverTap/clevertap-expo-plugin#readme",
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/react": "~18.3.12",
    "expo": "~52.0.0",
    "expo-build-properties": "^0.13.2",
    "expo-module-scripts": "^4.0.3",
    "expo-modules-core": "^2.2.1",
    "fs-extra": "^11.3.0",
    "react-native": "0.76.0"
  },
  "peerDependencies": {
    "expo": "*",
    "react": "*",
    "react-native": "*"
  }
}
